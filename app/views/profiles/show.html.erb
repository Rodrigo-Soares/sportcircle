<div class="page-header">

	<h1><%= @user.full_name %></h1>

</div>



	<% if @statuses %>
		
		<% @statuses.each do |status| %>
		<div class="well">
			<p><%= status.content %> <br \> <%= link_to time_ago_in_words(status.created_at) + " ago", status_path(status) %></p>
		</div>
		<% end %>

	<% end %>



